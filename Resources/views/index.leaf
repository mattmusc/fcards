#set("main") {
    <h1>FlashCards - #count(flashCards) cards</h1>

    <div>
        <button>Add general</button>
        <button>Add code</button>
    </div>

    <ul>
        #for(card in flashCards) {
            <li>
                <h4>#(card.title)</h4>
                <h5>#(card.content)</h5>
            </li>
        }
    </ul>
}

#embed("master")
